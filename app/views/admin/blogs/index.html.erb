<div class="newblogheader"><h1> Blog Verification </h1></div>
<br><br><br><br><br><br>
<% if @blogs.count != 0 %>
  <% @blogs.each do |blog| %>
    <div class="verifylist"><%= link_to blog.name, edit_admin_blog_path(blog) %></div>
  <% end %>
<% else %>
  <div class="newblogheader"><h2> No Blogs to Verify! </h2></div>
<% end %>
